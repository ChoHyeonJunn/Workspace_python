lst = list(range(1, 10))

print(list(lst[i] for i in range(4, 7)))

print([lst[i] for i in range(4, 7)])

print('[{},{},{}]'.format(lst[4], lst[5], lst[6]))

result = list();

for i in range(4, 7):
    result.append(lst[i])
else:
    print(result)

for i in range(4, 7):
    if i in lst:
        print('gg')

