lst = list(range(1, 10))

print('[{},{},{}]'.format(lst[4], lst[5], lst[6]))

for i in range(2, 10):
    print('<< {} ë‹¨ >>'.format(i))
    for j in range(1, 10):
        print('{} * {} = {}'.format(i, j, i * j))
    else:
        print()
    
mysum = lambda x, y:2 * x + y

print(mysum(2, 1))
