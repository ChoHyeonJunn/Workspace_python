# -*- coding:utf-8 -*-

# 2번
lst = list(range(1, 10))

print(list(lst[i] for i in range(4, 7)))

print([lst[i] for i in range(4, 7)])

print('[{}, {}, {}]'.format(lst[4], lst[5], lst[6]))

result = list();

for i in range(4, 7):
    result.append(lst[i])
else:
    print(result)

# 3번
for i in range(2, 10):
    print('<< {} 단 >>'.format(i))
    for j in range(1, 10):
        print('{} * {} = {}'.format(i, j, i * j))
    else:
        print()

# 4번 
mysum = lambda x, y:2 * x + y
 
print(mysum(2, 1))
