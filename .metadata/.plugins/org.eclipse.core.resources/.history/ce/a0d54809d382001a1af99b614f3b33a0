# -*- coding:utf-8 -*-

lst = list(range(1, 10))

print(list(lst[i] for i in range(4, 7)))
print('[{},{},{}]'.format(lst[4], lst[5], lst[6]))

str = '[';
for i in range(4, 7):
    str += lst[i]
else:
    str += ']'
    print(str)

